  <xs:simpleType name="aobt_clock_start_count">
    <xs:annotation>
      <xs:documentation>The aobt_clock_start_count specifies the number
        of 1/1024 seconds recorded on the APSS On Board Time clock at
        the beginning of an observation.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="pds:ASCII_NonNegative_Integer">
    	<xs:pattern value='[0-9]+'/>
     <xs:minInclusive value="0"/>
     <xs:maxInclusive value="18446744073709551615"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="aobt_clock_stop_count">
    <xs:annotation>
      <xs:documentation>The aobt_clock_stop_count specifies the number
        of 1/1024 seconds recorded on the APSS On Board Time clock at
        the end of an observation.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="pds:ASCII_NonNegative_Integer">
