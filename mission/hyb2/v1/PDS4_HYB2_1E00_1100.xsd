     <xs:minLength value="1"/>
     <xs:maxLength value="255"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="heater_current_average_WO_Units">
    <xs:restriction base="pds:ASCII_Real">
